@model IEnumerable<KAR.KSFC.Components.Common.Dto.EnquirySubmission.UnitDetails.RegistrationNoDetailsDTO>

<h3><i class="fas fa-comments-dollar text-success"></i> Registration Details</h3>
    @if (Model != null)
{
    <table class="table table-wrapper">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.EnqRegnoId)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.RegrefCd)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.EnqRegno)
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.EnqRegnoId)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.RegrefCd)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.EnqRegno)
                    </td>
                </tr>
            }
        </tbody>
    </table>
}
@if (Model == null)

{
    <p style="color: red"><b>No data added, Please add the Registration details</b></p>
}

