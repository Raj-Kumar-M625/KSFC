@model IEnumerable<KAR.KSFC.Components.Common.Dto.EnquirySubmission.ProjectDetails.ProjectMeansOfFinanceDTO>


<h3><i class="fas fa-comments-dollar text-success"></i> Means of Finance (in Lakhs)</h3>
    @if (Model != null)
{
    <table class="table table-wrapper">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.PjmfCd)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.MfcatCd)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.MfcatCdNavigation.PjmfDets)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.EnqPjmfValue)
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.PjmfCd)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.MfcatCd)
                    </td>
                    <td>
                        
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.EnqPjmfValue)
                    </td>
                </tr>
            }
        </tbody>
    </table>
}
@if (Model == null)

{
    <p style="color: red"><b>No data added, Please add the Means of Finance details</b></p>
}


