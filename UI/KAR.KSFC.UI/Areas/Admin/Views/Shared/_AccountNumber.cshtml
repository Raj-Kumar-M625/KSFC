        <strong><label class="col-md-2 ms-5 textleft2">Inspection Number:</label></strong>
        <input class="col-md-1 btnDet20" type="text" value="@ViewBag.inspectonID" disabled />
        <br />
        <div style="margin-top: 10px;">
        <strong><label class="col-md-2 textleft2">Name of Official :</label></strong>
        <input class="col-md-2 btnDet20" type="text" value="@ViewBag.officialName" disabled />
        </div>
        <br />
        <hr />
    </div>
}
else
{
    <div >
    <br />
    <strong><label class="col-md-2 textleft2" >Loan Account No. :</label></strong>
    <input class="col-md-2 btnDet17" type="text" value="@ViewBag.AccountNumber" disabled/>  @*// Modified by Dev on 05/08/2022*@
