    <strong><label class="col-md-2 textleft2" >Loan Sub :</label></strong>
    <input class="col-md-2 btnDet18" type="text" value="@ViewBag.LoanSub" disabled/>  @*// Added by Dev on 05/08/2022*@
    &emsp;&emsp;
    <strong><label class="col-md-2 textleft2" >Name of Unit :</label></strong>
        <input class="col-md-2 btnDet19" type="text" id="ChangedUnitName" value="@ViewBag.UnitName" disabled />    @*// Modified by Dev on 05/08/2022*@
    <br />
    <hr/>
</div>
    
}

