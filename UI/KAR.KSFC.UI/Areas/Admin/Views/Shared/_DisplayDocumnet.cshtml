@model List<KAR.KSFC.Components.Common.Dto.IDM.ldDocumentDto>
@{
    var subModuleId = ViewBag.SubModuleId;
    var subModuleType = ViewBag.SubModuleType;
    var mainModule = ViewBag.MainModule;
}
        


<a onclick="showDocumentPopup('@Url.Action("DisplayDocument","IdmDocument",new {submoduleId=@subModuleId,submoduleType=@subModuleType,mainModule=@mainModule},Context.Request.Scheme)','Uploaded Documents')" class="btn btn-primary text-white"> Upload / View Document(s)</a>
<div class="modal fade" tabindex="-1" role="dialog" id="viewDocuments">
    <div class="modal-dialog modal-dialog-scrollable modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header" id="docHeader">
                <h5 class="modal-title"></h5>
                <button type="button" class="close" data-dismiss="modal" onclick="CloseDocPopupFormsh();" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
            </div>
        </div>
    </div>
</div>
<style>
    .modal-content{
        border-color: #2c3e50;
    }
</style>

