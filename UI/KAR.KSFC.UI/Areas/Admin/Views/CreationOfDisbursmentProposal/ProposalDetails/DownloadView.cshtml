@model KAR.KSFC.Components.Common.Dto.IDM.CreationOfDisbursmentProposal.TblIdmReleDetlsDTO
@{
    Layout = null;
}
<style>
#data {
  font-family: Arial, Helvetica, sans-serif;
  border-collapse: collapse;
  width: 100%;
}

#data td, #data th {
  
  padding: 8px;
}

#data tr:nth-child(even){background-color: #f2f2f2;}

#data tr:hover {background-color: #ddd;}

#data th {
  padding-top: 12px;
  padding-bottom: 12px;
  text-align: left;
  background-color: #04AA6D;
  color: white;
}
</style>
<div>
    <div>
      <div class="row">
                <div class="col-md-4">
                    <strong><label class="form-label">Proposal No.</label></strong>
                    <input type="Number" asp-for="TblIdmDisbProp.PropNumber" class="form-control" min="0" readonly onkeypress="return event.charCode >= 46 && event.charCode <= 57" />
                    <span asp-validation-for="TblIdmDisbProp.PropNumber" class="text-danger"></span>

                </div>
                <div class="col-md-4">
                    <strong><label class="form-label">Department</label></strong>
                @* @Html.TextAreaFor(m => m.AuditCompliance, new { @class = "form-control", @maxlength = "150", @id="auditcondtion" })
                    <span asp-validation-for="AuditObservation" class="text-danger"></span>*@
                    <input type="Number" class="form-control" disabled="disabled" min="0" onkeypress="return event.charCode >= 46 && event.charCode <= 57" readonly />

                </div>
                <div class="col-md-4">
                    <strong><label calss="from-label">At per Charges<span id="spanDet1"> *</span></label></strong>
                <input type="Number" asp-for="ReleAtParCharges" class="form-control" min="0" onkeypress="return event.charCode >= 46 && event.charCode <= 57" readonly style="text-align:right" />
                    <span asp-validation-for="ReleAtParCharges" class="text-danger"></span>
                </div>

            </div>
            <br />
            <div class="row">
                <div class="col-md-4">
                <strong><label calss="from-label">Release Amount<span id="spanDet1"> *</span></label></strong>
                    <input type="Number" asp-for="ReleAmount" class="form-control" min="0" readonly onkeypress="return event.charCode >= 46 && event.charCode <= 57" style="text-align:right"/>
                    <span asp-validation-for="ReleAmount" class="text-danger"></span>
                </div>
                <div class="col-md-4">
                <strong><label calss="from-label">Dues Amount<span id="spanDet1"> *</span></label></strong>
                <input type="Number" asp-for="ReleDueAmount" class="form-control" min="0" onkeypress="return event.charCode >= 46 && event.charCode <= 57" readonly style="text-align:right" />
                    <span asp-validation-for="ReleDueAmount" class="text-danger"></span>
                </div>
                <div class="col-md-4">
                <strong><label calss="from-label">Document Charges<span id="spanDet1"> *</span></label></strong>
                <input type="Number" asp-for="ReleDocChg" class="form-control" min="0" onkeypress="return event.charCode >= 46 && event.charCode <= 57" readonly style="text-align:right" />
                    <span asp-validation-for="ReleDocChg" class="text-danger"></span>
                </div>

            </div>
            <br />
            <div class="row">
                <div class="col-md-4">
                <strong><label calss="from-label">Adjustment Amount<span id="spanDet1"> *</span></label></strong>
                <input type="Number" asp-for="ReleAdjAmount" class="form-control" min="0" readonly onkeypress="return event.charCode >= 46 && event.charCode <= 57" style="text-align:right" />
                    <span asp-validation-for="ReleAdjAmount" class="text-danger"></span>
                </div>
                <div class="col-md-4">
                <strong><label calss="from-label">FD Amount<span id="spanDet1"> *</span></label></strong>
                <input type="Number" asp-for="ReleFdAmount" class="form-control" min="0" onkeypress="return event.charCode >= 46 && event.charCode <= 57" readonly style="text-align:right" />
                    <span asp-validation-for="ReleFdAmount" class="text-danger"></span>
                </div>
                <div class="col-md-4">
                <strong><label calss="from-label">Commitment Charges<span id="spanDet1"> *</span></label></strong>
                <input type="Number" asp-for="ReleComChg" class="form-control" min="0" onkeypress="return event.charCode >= 46 && event.charCode <= 57" readonly style="text-align:right" />
                    <span asp-validation-for="ReleComChg" class="text-danger"></span>
                </div>
            </div>
            <br />
            <div class="row">
                <div class="col-md-4">
                <strong><label class="form-label">Sequence No.<span id="spanDet1"> *</span></label></strong>
                    <input type="Number" asp-for="ReleAdjRecSeq" class="form-control" min="0" onkeypress="return event.charCode >= 46 && event.charCode <= 57" readonly style="text-align:right"/>
                    <span asp-validation-for="ReleAdjRecSeq" class="text-danger"></span>
                </div>
                <div class="col-md-4">
                <strong><label calss="from-label">Upfront Fees<span id="spanDet1"> *</span></label></strong>
                    <input type="Number" asp-for="ReleAddUpFrtAmount" class="form-control" min="0" onkeypress="return event.charCode >= 46 && event.charCode <= 57" readonly style="text-align:right"/>
                    <span asp-validation-for="ReleAddUpFrtAmount" class="text-danger"></span>
                </div>
                <div class="col-md-4">
                <strong><label calss="from-label">Bank Charges<span id="spanDet1"> *</span></label></strong>
                <input type="Number" asp-for="ReleBnkChg" class="form-control" min="0" onkeypress="return event.charCode >= 46 && event.charCode <= 57" readonly style="text-align:right" />
                    <span asp-validation-for="ReleBnkChg" class="text-danger"></span>
                </div>
            </div>
            <br />
            <div class="row">
                <div class="col-md-4">
                    <strong><label calss="from-label">Total Amount</label></strong>
                <input type="Number" class="form-control" min="0" onkeypress="return event.charCode >= 46 && event.charCode <= 57" readonly style="text-align:right" />

                </div>
            </div>
    </div>
</div>